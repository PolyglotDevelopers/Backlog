@model SettingModel
@Html.AntiForgeryToken()
<input type="hidden" asp-for="Id" />
<div class="row">
    <div class="col-md-12">
        <div class="mb-3">
            <label asp-for="Name" class="form-label"></label>
            <input asp-for="Name" class="form-control form-control-maxlength" autocomplete="off" readonly="readonly" />
            <span asp-validation-for="Name" class="text-danger"></span>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <div class="mb-3">
            <label asp-for="Description" class="form-label"></label>
            <input asp-for="Description" class="form-control form-control-maxlength" autocomplete="off" maxlength="250" />
            <span asp-validation-for="Description" class="text-danger"></span>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <div class="mb-3">
            <label asp-for="Value" class="form-label"></label>
            <input asp-for="Value" class="form-control form-control-maxlength" autocomplete="off" maxlength="250" />
            <span asp-validation-for="Value" class="text-danger"></span>
        </div>
    </div>
</div>