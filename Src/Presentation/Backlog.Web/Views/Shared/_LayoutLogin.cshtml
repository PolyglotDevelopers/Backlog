<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0" />
    <title>@ViewData["Title"] - Backlog</title>
    <!-- Favicon -->
    <link rel="icon" href="~/images/brand/favicon.ico" type="image/x-icon" />
    <!-- Core CSS -->
    <link rel="stylesheet" href="~/css/theme/tabler.min.css" class="template-customizer-core-css" />
    <!-- Application css -->
    <link rel="stylesheet" href="~/css/login.css" asp-append-version="true" />
</head>

<body class="d-flex flex-column">
    <div class="page page-center">
        @RenderBody()
    </div>
    <!-- Core JS -->
    <script src="~/js/theme/tabler.min.js"></script>
    <!-- Plugin JS -->
    <script src="~/lib/jquery/jquery.min.js"></script>
    <script src="~/lib/jquery-validation/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>
